@using TutoringCenter.VN.Models
@model AboutUs
@{
    Layout = "~/Views/Shared/_MetaLayout.cshtml";
    ViewData["Title"] = "Index";
    IEnumerable<AboutUs> aboutUs = ViewBag.aboutUs;
}
    <section class="aboutUs-section">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <h2 style="color: #ffffff; background-color: #802eb7"><strong>Về chúng tôi</strong></h2>
                    @for (int i = 0; i < aboutUs.Count(); i++)
                    {
                        AboutUs aboutUss = aboutUs.ElementAt(i);
                        @Html.Raw(@aboutUss.Content);
                    }
                </div>
            </div>
        </div>
    </section>

